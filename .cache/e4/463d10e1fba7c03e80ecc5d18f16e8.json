s/ConductTransaction.js","original":{"line":64,"column":8}},{"generated":{"line":121,"column":19},"source":"components/ConductTransaction.js","original":{"line":40,"column":6}},{"generated":{"line":121,"column":20},"source":"components/ConductTransaction.js","original":{"line":39,"column":4}},{"generated":{"line":122,"column":0},"source":"components/ConductTransaction.js","original":{"line":67,"column":3}},{"name":"Component","generated":{"line":126,"column":2},"source":"components/ConductTransaction.js","original":{"line":7,"column":48}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from 'react'\nimport { FormGroup, FormControl } from 'react-bootstrap'\nimport { Button } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport history from '../history';\n\nexport default class ConductTransaction extends Component {\n\n    state =  {   recipient:'', amount: 0 }\n\n    updateRecipient = event => {\n\n        this.setState({ recipient: event.target.value })\n   }\n\n\n    updateAmount = event => {\n\n            this.setState({ amount: Number(event.target.value)})\n    }\n\n    conductTransaction = () => {\n\n        const { recipient, amount } = this.state\n\n        fetch(`${document.location.origin}/api/transact`, {\n            method:'POST',\n            headers: { 'Content-Type': 'application/json'},\n            body: JSON.stringify({ recipient, amount}) })\n            .then( response => response.json()).then(json => { alert(json.message || json.type )\n                })\n    \n                    history.push('/transaction-pool')\n            }\n    \n  render() {\n\n    console.log('this.state', this.state)\n    return (\n      <div className=\"ConductTransaction\">\n          <div>\n        <Link to=\"/\">Home</Link>\n        </div>\n        <h3>Conduct Transaction</h3>\n\n        <FormGroup>\n            <FormControl\n            input=\"text\"\n            placeholder='recipient'\n            value={this.state.recipient}\n            onChange={this.updateRecipient}\n            />\n        </FormGroup>\n\n        <FormGroup>\n\n        <FormControl\n            input=\"number\"\n            placeholder='amount'\n            value={this.state.amount}\n            onChange={this.updateAmount} \n            />   \n        </FormGroup>\n        <div><Button onClick={this.conductTransaction} bsStyle =\"danger\">Submit</Button></div>\n      </div>\n    )\n  }\n}\n\n"},"lineCount":null}},"hash":"abdf91aa5f644a5ce3272e404748df17","cacheData":{"env":{}}}